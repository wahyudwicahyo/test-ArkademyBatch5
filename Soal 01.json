def biodata (name, age, address, hobbies, is_married, list_school, skills, interest_in_coding):

    MyName = name
    MyAge = age
    MyAdress = address
    MyHobby = hobbies
    MyStatus = is_married
    MySchool = list_school
    MySkill = skills
    MyInterest = interest_in_coding

    return MyName, MyAge, MyAdress, MyHobby, MyStatus, MySchool, MySkill, MyInterest

